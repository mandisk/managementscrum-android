<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="LWFP">
	    <item name="android:layout_width">fill_parent</item>
	</style>	
	<style name="LWFP.LHFP">
	    <item name="android:layout_height">fill_parent</item>
	</style>
	<style name="LWFP.LHFP.PAD20">
	    <item name="android:paddingLeft">20dp</item>
	    <item name="android:paddingRight">20dp</item>
	    <item name="android:paddingBottom">20dp</item>
	</style>
	<style name="LWFP.LHFP.PAD20.OV">
	    <item name="android:orientation">vertical</item>
	</style>
	<style name="LWFP.LHFP.PAD20.OV.all">
	    <item name="android:paddingTop">20dp</item>
	    
	</style>
	
	<style name="LWMP">
	    <item name="android:layout_width">match_parent</item>
	</style>
	<style name="LWMP.LHMP">
	    <item name="android:layout_height">match_parent</item>
	</style>
	<style name="LWMP.LHMP.OV">
	    <item name="android:orientation">vertical</item>
	</style>
	
	<style name="LWMP.LHWC">
	    <item name="android:layout_height">wrap_content</item>
	</style>
	<style name="LWMP.LHWC.OV">
	    <item name="android:orientation">vertical</item>
	</style>
	<style name="LWMP.LHWC.GCH">
	    <item name="android:gravity">center_horizontal</item>
	</style>
	<style name="LWMP.LHWC.OV.CH">
	    <item name="android:gravity">center_horizontal</item>
	</style>
	
	<style name="LWFP.LHWC">
	    <item name="android:layout_height">wrap_content</item>
	</style>
	<style name="LWFP.LHWC.OV">
	    <item name="android:orientation">vertical</item>
	</style>
	<style name="LWFP.LHWC.OH">
	    <item name="android:orientation">horizontal</item>
	</style>
	<style name="LWFP.LHWC.OH.MB10">
		<item name="android:layout_marginBottom">10dp</item>
	</style>
	<style name="LWWC">
	    <item name="android:layout_width">wrap_content</item>
	</style>
	<style name="LWWC.LHWC">
	    <item name="android:layout_height">wrap_content</item>
	</style>
	
	<style name="ET">
	    <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:typeface">normal</item>
	    <item name="android:layout_marginBottom">10dp</item>
	</style>
	<style name="ET.T">
	    <item name="android:inputType">text</item>
	</style>
	<style name="ET.E">
	    <item name="android:inputType">textEmailAddress</item>
	</style>
	<style name="ET.P">
	    <item name="android:inputType">textPassword</item>
	</style>
	<style name="ET.N">
	    <item name="android:inputType">numberDecimal</item>
	</style>
	<style name="ET.M">
	    <item name="android:inputType">textMultiLine</item>
	</style>
	
	<style name="BUTTON_MW" >
	    <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:typeface">normal</item>
	    <item name="android:layout_marginTop">20dp</item>
	    <item name="android:textSize">18dp</item>
	</style>
	<style name="BUTTON_WW">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:typeface">normal</item>
	    <item name="android:layout_marginTop">10dp</item>
	    <item name="android:textSize">18dp</item>
	</style>
	
	<style name="TV" parent="@android:style/TextAppearance.Medium">
	    <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:typeface">normal</item>
	</style>
	<style name="TV.P9DP">
	    <item name="android:padding">9dp</item>
	</style>
	<style name="TV.list">
	    <item name="android:textColor">#FF88FF</item>
	    <item name="android:paddingBottom">6dp</item>
	    <item name="android:paddingTop">6dp</item>
	</style>
	<style name="TV.listSub">
	    <item name="android:paddingBottom">3dp</item>
	</style>
	<style name="TVWCWC" parent="@android:style/TextAppearance.Medium">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:typeface">normal</item>
	    <item name="android:padding">9dp</item>
	</style>
	
	<style name="TVHEADER" parent="@android:style/TextAppearance.Large">
	    <item name="android:layout_width">wrap_content</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:typeface">normal</item>
	</style>
	
	<style name="LWWC.LHWC.IMAGE">
	    <item name="android:padding">3dp</item>
	</style>
	
	<style name="userPhoto">
	    <item name="android:layout_width">250dp</item>
	    <item name="android:layout_height">250dp</item>
	    <item name="android:paddingTop">9dp</item>
	    <item name="android:gravity">center_horizontal</item>
	</style>
	
	<style name="CB" parent="@android:style/TextAppearance.Medium">
	    <item name="android:layout_width">match_parent</item>
	    <item name="android:layout_height">wrap_content</item>
	    <item name="android:typeface">normal</item>
	    <item name="android:paddingBottom">10dp</item>
	    <item name="android:paddingTop">10dp</item>
	</style>
</resources>